    "compilerOptions": {
        "target": "es2016",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "module": "CommonJS",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "allowJs": true,
        "outDir": "target"
    }
}